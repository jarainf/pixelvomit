{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build pixelvomit",
            "type": "shell",
            "command": "gcc",
            "args": [
                "pixelvomit.c",
                "-o",
                "pixelvomit",
                "-O3",
                "-pthread"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": [
                "$gcc"
            ],
            "detail": "Generated task for building the pixelvomit program"
        },
        {
            "label": "build debug pixelvomit",
            "type": "shell",
            "command": "gcc",
            "args": [
                "pixelvomit.c",
                "-fdiagnostics-color=always",
                "-g",
                "-o",
                "pixelvomit",
                "-O3",
                "-pthread"
            ],
            "group": "build",
            "problemMatcher": [
                "$gcc"
            ],
            "detail": "Generated task for building the pixelvomit program"
        },
    ]
